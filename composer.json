{
	"name": "blitze/sitemaker",
    "type": "phpbb-extension",
	"description": "A Site Maker for phpBB3.1x.",
	"homepage": "https://github.com/blitze/phpBB-ext-sitemaker",
	"version": "2.0.3",
	"time": "2014-01-14 01:01:01",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Daniel A.",
			"email": "shizzah@hotmail.com",
			"homepage": "https://github.com/blitze",
			"role": "Lead Developer"
		}],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0",
		"nickvergessen/phpbb-tool-trimmessage": "^1.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "phpBB SiteMaker",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.0@dev"
		},
		"version-check": {
			"host": "blitze.github.io",
			"directory": "/versions",
			"filename": "sitemaker.json"
		}
	}
}
